{% extends 'base.html.twig' %}

{% block title %}{{parent() }} - Option{% endblock %}

{% block body %}
<div class="container">

    <h1>Option</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ option.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ option.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_option_index') }}">back to list</a>

    <a href="{{ path('admin_option_edit', {'id': option.id}) }}">edit</a>

    {{ include('admin/option/_delete_form.html.twig') }}
</div>

{% endblock %}